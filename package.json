{
  "name": "@obinexuscomputing/hypernum",
  "version": "0.1.0",
  "description": "A high-precision mathematics library for large number operations with BigInt support, custom data structures, and comprehensive type safety",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "cdn": "dist/index.umd.js",
  "sideEffects": false,
  
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "rollup.config.js"
  ],
  
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist coverage .nyc_output",
    "prebuild": "npm run clean",
    "prepack": "npm run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --runInBand",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm run test:ci && npm run lint",
    "docs": "typedoc --out docs src",
    "benchmark": "ts-node benchmark/index.ts",
    "release": "standard-version",
    "commit": "git-cz"
  },
  
  "keywords": [
    "bigint",
    "large-numbers",
    "arithmetic",
    "mathematics",
    "computation",
    "data-structures",
    "typescript",
    "javascript",
    "number-theory",
    "high-precision",
    "ackermann",
    "tetration",
    "avl-tree",
    "power-tower"
  ],
  
  "author": {
    "name": "Obi Nexus Computing",
    "email": "contact@obinexuscomputing.com",
    "url": "https://obinexuscomputing.com"
  },
  
  "funding": {
    "type": "individual",
    "url": "https://www.buymeacoffee.com/obinexuscomputing"
  },
  
  "license": "ISC",
  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obinexuscomputing/hypernum.git"
  },
  
  "bugs": {
    "url": "https://github.com/obinexuscomputing/hypernum/issues"
  },
  
  "homepage": "https://obinexuscomputing.github.io/hypernum",
  
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  
  "dependencies": {
    "tslib": "^2.6.0"
  },
  
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  
  "engines": {
    "node": ">=16.0.0"
  },
  
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80
  }
}